<!-- BEGIN preview -->
<table class="forumline" width="100%" cellspacing="1" cellpadding="4" border="0">
	<tr> 
		<th height="25" class="thHead">{preview.L_PREVIEW}</th>
	</tr>
	<tr>
		<td class="row1"><span class="postbody">
			<!-- BEGIN msg_icon --><img src="{preview.msg_icon.I_ICON}" border="0" title="{preview.msg_icon.L_ICON}" class="absbottom" alt="" />&nbsp;<!-- END msg_icon -->
			<!-- BEGIN sub_type --><!-- BEGIN img --><img src="{preview.sub_type.I_SUB_TYPE}" border="0" alt="{preview.sub_type.L_SUB_TYPE}" title="{preview.sub_type.L_SUB_TYPE}" class="absbottom" /><!-- END img --><!-- BEGIN txt --><b>[{preview.sub_type.L_SUB_TYPE}]</b><!-- END txt -->&nbsp;<!-- END sub_type -->
			<!-- BEGIN title --><b>{preview.POST_SUBJECT}</b><br /></span><span class="postdetails"><!-- END title -->
			<!-- BEGIN sub_title -->{preview.sub_title.SUB_TITLE}<br /><!-- END sub_title -->
			<!-- BEGIN announce -->{preview.announce.S_ANNOUNCE}<br /><!-- END announce -->
			<!-- BEGIN calendar_event -->{preview.calendar_event.S_CALENDAR_EVENT}<br /><!-- END calendar_event -->
			<!-- BEGIN title --></span><hr /><span class="postbody"><!-- END title -->
			{preview.MESSAGE}<br />
		</span>
		{preview.ATTACHMENTS}
		<div class="postbody">
			<!-- BEGIN signature -->_________________<br />{preview.SIGNATURE}<br /><!-- END signature -->
		</div><div class="postdetails" align="right"><br />
			<img src="{preview.I_POST}" alt="{preview.L_POST}" border="0" hspace="2" /><b>{preview.L_POSTED}:</b>&nbsp;{preview.POST_DATE}<br />
		</div></td>
	</tr>
	<tr> 
		<td class="spaceRow" height="1"><img src="{I_SPACER}" width="1" height="1" alt="" /></td>
	</tr>
</table>

<br clear="all" />
<!-- END preview -->